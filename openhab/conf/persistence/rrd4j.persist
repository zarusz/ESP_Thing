Strategies {
	everyMinute:"0 * * * * ?"
    everyHour : "0 0 * * * ?"
    everyDay  : "0 0 0 * * ?"
	every15M  : "0 */15 0 * * ?"
    default = everyChange
}

Items {
    Temp, CTemp, Bedroom_Temp : strategy = every15M, restoreOnStartup
    Humid, CHumid, Bedroom_Humid : strategy = every15M, restoreOnStartup
}